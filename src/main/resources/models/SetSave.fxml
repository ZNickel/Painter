<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" styleClass="Background" stylesheets="@../styles/dark.css"
            xmlns="http://javafx.com/fxml" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="zn.soft.logic.controllers.SetSaveControl">
   <HBox layoutX="76.0" layoutY="77.0" styleClass="BorderTop" AnchorPane.bottomAnchor="0.0"
         AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
      <Button mnemonicParsing="false" onAction="#pressed_Cancel" prefWidth="3000.0" text="Cancel">
         <styleClass>
            <String fx:value="Btn"/>
            <String fx:value="RedText"/>
         </styleClass>
      </Button>
      <Button mnemonicParsing="false" onAction="#pressed_Save" prefWidth="3000.0" text="Save">
         <styleClass>
            <String fx:value="Btn"/>
            <String fx:value="GreenText"/>
         </styleClass>
      </Button>
   </HBox>
   <VBox alignment="CENTER" layoutX="115.0" layoutY="-24.0" spacing="5.0" AnchorPane.bottomAnchor="24.0"
         AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <HBox alignment="CENTER">
         <TextField fx:id="tf_SetName" alignment="CENTER" prefWidth="200.0" promptText="name...">
            <styleClass>
               <String fx:value="TFSilent"/>
               <String fx:value="BorderBottom"/>
            </styleClass>
         </TextField>
      </HBox>
   </VBox>
</AnchorPane>

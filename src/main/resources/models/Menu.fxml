<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="pane" prefHeight="400.0" prefWidth="600.0" styleClass="Background" stylesheets="@../styles/dark.css"
            xmlns="http://javafx.com/fxml" xmlns:fx="http://javafx.com/fxml"
            fx:controller="zn.soft.logic.controllers.MenuControl">
   <VBox alignment="CENTER" layoutX="200.0" layoutY="88.0" prefHeight="200.0" prefWidth="100.0"
         AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
         AnchorPane.topAnchor="0.0">
      <Button mnemonicParsing="false" onAction="#pressed_Manual" prefHeight="30.0" prefWidth="200.0" styleClass="Btn"
              text="Manual"/>
      <Button mnemonicParsing="false" onAction="#pressed_Auto" prefHeight="30.0" prefWidth="200.0" styleClass="Btn"
              text="Auto"/>
      <Button mnemonicParsing="false" onAction="#pressed_FullAuto" prefHeight="30.0" prefWidth="200.0"
              styleClass="Btn" text="FullAuto"/>
      <Button mnemonicParsing="false" onAction="#pressed_EditPresets" prefHeight="30.0" prefWidth="200.0"
              styleClass="Btn" text="Edit presets"/>
      <Button mnemonicParsing="false" onAction="#pressed_Options" prefHeight="30.0" prefWidth="200.0"
              styleClass="Btn" text="Options"/>
      <Button mnemonicParsing="false" onAction="#pressed_Exit" prefHeight="30.0" prefWidth="200.0" text="Exit">
         <styleClass>
            <String fx:value="RedText"/>
            <String fx:value="Btn"/>
         </styleClass>
      </Button>
   </VBox>
</AnchorPane>
